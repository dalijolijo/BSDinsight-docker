{
  "network": "livenet",
  "port": 3001,
  "services": [
    "bitcoind",
    "insight-api-bsd",
    "insight-ui-bsd",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "/home/bitsend/bitcore-livenet/bin/mynode/data",
        "exec": "/home/bitsend/bitcore-livenet/bin/bitsendd"
      }
    },
    "insight-api-bsd": {
        "routePrefix": "api"
    },
    "insight-ui-bsd": {
        "apiPrefix": "api",
        "routePrefix": "joli"
    }
  }
}
